#include <iostream>
using namespace std;
int main()
{
int angka;
cout<<"---------------------------
"<<endl; cout<<"Daftar Minuman"<<endl;
cout<<"1.Tebs"<<endl;
cout<<"2.Cimory"<<endl;
cout<<"3.Pulpy"<<endl;
cout<<"4.You.C100"<<endl;
cout<<"5.Tripanca"<<endl;
cout<<"---------------------------
"<<endl; cout<<"Masukkan Minuman Anda "
<<endl; cin>>angka;
if (angka==1)
cout<<"Minuman Anda Tebs,Minuman Tebs Akan Keluar";
else
if (angka==2)
cout<<"Minuman Anda Cimory,Minuman Cimory Akan Keluar";
else
if (angka==3)
cout<<"Minuman Anda Pulpy,Minuman Pulpy Akan Keluar";
else
if (angka==4)
cout<<"Minuman Anda You.C100,Minuman You.C100 Akan Keluar";
else
if (angka==5)
cout<<"Minuman Anda Tripanca,Minuman Tripanca Akan Keluar ";
else
cout<<"Anda Memasukkan Angka Yang Salah ";
return 0;
}
